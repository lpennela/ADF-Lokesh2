{
	"name": "DS_SinkDataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_BLOB",
			"type": "LinkedServiceReference"
		},
		"folder": {
			"name": "Incremental_Load"
		},
		"type": "AzureBlob",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"fileName": {
				"value": "@concat('IncrementalLoad_',pipeline().RunId,'.txt')",
				"type": "Expression"
			},
			"folderPath": "lokesh/incrementalcopy"
		}
	}
}