{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "DF-Lokesh2"
		},
		"LS_ADLS_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'LS_ADLS'"
		},
		"LS_ASQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ASQL'"
		},
		"LS_BLOB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BLOB'"
		},
		"DS_ADLS_EMP_Max_SRC_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@dataset().FileName"
		},
		"DS_ADLS_EMP_Max_SRC_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "EMP/Employees"
		},
		"DS_ADLS_EMP_Max_TGT_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"DS_ADLS_EMP_Max_TGT_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "Outbound/Emp_Max"
		},
		"DS_ADLS_Files_SRC_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"DS_ADLS_Files_SRC_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "EMP/Employees"
		},
		"DS_ADLS_Upsert_Employee_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "Employee.txt"
		},
		"DS_ADLS_Upsert_Employee_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "EMP"
		},
		"DS_BLOB_Event_SRC_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "*.txt"
		},
		"DS_BLOB_Event_SRC_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "lokesh"
		},
		"DS_BLOB_Event_TGT_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"DS_BLOB_Event_TGT_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "lokesh/Outbound"
		},
		"LS_ADLS_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://adlslokesh2.azuredatalakestore.net/webhdfs/v1"
		},
		"LS_ADLS_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "506f7cec-1d8f-4578-87a5-5d73dd551530"
		},
		"LS_ADLS_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "59843051-b9bd-40fa-8400-b19d744add1c"
		},
		"LS_ADLS_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "cb8eca64-bfd6-4bd8-b710-6e1f30e399d1"
		},
		"LS_ADLS_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "RG_Lokesh"
		},
		"Trigger_BLOB_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/cb8eca64-bfd6-4bd8-b710-6e1f30e399d1/resourceGroups/RG_Lokesh/providers/Microsoft.Storage/storageAccounts/storageaccountlokesh2"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_servicePrincipalKey": {
						"value": "[parameters('LS_ADLS_servicePrincipalKey')]"
					},
					"LS_ASQL_connectionString": {
						"value": "[parameters('LS_ASQL_connectionString')]"
					},
					"LS_BLOB_connectionString": {
						"value": "[parameters('LS_BLOB_connectionString')]"
					},
					"LS_ADLS_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_dataLakeStoreUri')]"
					},
					"LS_ADLS_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_servicePrincipalId')]"
					},
					"LS_ADLS_properties_typeProperties_tenant": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_tenant')]"
					},
					"LS_ADLS_properties_typeProperties_subscriptionId": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_subscriptionId')]"
					},
					"LS_ADLS_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_resourceGroupName')]"
					},
					"DS_ADLS_EMP_Max_SRC_properties_typeProperties_fileName": {
						"value": "[parameters('DS_ADLS_EMP_Max_SRC_properties_typeProperties_fileName')]"
					},
					"DS_ADLS_EMP_Max_SRC_properties_typeProperties_folderPath": {
						"value": "[parameters('DS_ADLS_EMP_Max_SRC_properties_typeProperties_folderPath')]"
					},
					"DS_ADLS_EMP_Max_TGT_properties_typeProperties_fileName": {
						"value": "[parameters('DS_ADLS_EMP_Max_TGT_properties_typeProperties_fileName')]"
					},
					"DS_ADLS_EMP_Max_TGT_properties_typeProperties_folderPath": {
						"value": "[parameters('DS_ADLS_EMP_Max_TGT_properties_typeProperties_folderPath')]"
					},
					"DS_ADLS_Files_SRC_properties_typeProperties_fileName": {
						"value": "[parameters('DS_ADLS_Files_SRC_properties_typeProperties_fileName')]"
					},
					"DS_ADLS_Files_SRC_properties_typeProperties_folderPath": {
						"value": "[parameters('DS_ADLS_Files_SRC_properties_typeProperties_folderPath')]"
					},
					"DS_ADLS_Upsert_Employee_properties_typeProperties_fileName": {
						"value": "[parameters('DS_ADLS_Upsert_Employee_properties_typeProperties_fileName')]"
					},
					"DS_ADLS_Upsert_Employee_properties_typeProperties_folderPath": {
						"value": "[parameters('DS_ADLS_Upsert_Employee_properties_typeProperties_folderPath')]"
					},
					"DS_BLOB_Event_SRC_properties_typeProperties_fileName": {
						"value": "[parameters('DS_BLOB_Event_SRC_properties_typeProperties_fileName')]"
					},
					"DS_BLOB_Event_SRC_properties_typeProperties_folderPath": {
						"value": "[parameters('DS_BLOB_Event_SRC_properties_typeProperties_folderPath')]"
					},
					"DS_BLOB_Event_TGT_properties_typeProperties_fileName": {
						"value": "[parameters('DS_BLOB_Event_TGT_properties_typeProperties_fileName')]"
					},
					"DS_BLOB_Event_TGT_properties_typeProperties_folderPath": {
						"value": "[parameters('DS_BLOB_Event_TGT_properties_typeProperties_folderPath')]"
					},
					"Trigger_BLOB_properties_typeProperties_scope": {
						"value": "[parameters('Trigger_BLOB_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}